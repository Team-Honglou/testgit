GRAFANA_USER=logdisplayplatform

GRAFANA_GROUP=logdisplayplatform

GRAFANA_HOME=/usr/share/logdisplayplatform

LOG_DIR=/var/log/logdisplayplatform

DATA_DIR=/var/lib/logdisplayplatform

MAX_OPEN_FILES=10000

CONF_DIR=/etc/logdisplayplatform

CONF_FILE=/etc/logdisplayplatform/logdisplayplatform.ini

RESTART_ON_UPGRADE=true

PLUGINS_DIR=/var/lib/logdisplayplatform/plugins

PROVISIONING_CFG_DIR=/etc/logdisplayplatform/provisioning

# Only used on systemd systems
PID_FILE_DIR=/var/run/logdisplayplatform
